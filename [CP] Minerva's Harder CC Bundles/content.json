{
	"Format": "1.10",

	"ConfigSchema": {

		"DifficultyLevel": {
			"AllowValues": "Easy, Regular, Hard, OJRebalance",
			"Default": "Regular",
			"AllowBlank": false
			},
		"PostCCBundleChanges": {
			"AllowValues": "true, false",
			"Default": true,
			"AllowBlank": false
			}
	},

